<mapper namespace="signInMapper" >
    <select id="getPassword" resultType="ac.dankook.dht2.data.User">
      SELECT user_password
      FROM USERDATA
      where user_id=#{id};
    </select>
    <insert id="insertData">
        INSERT INTO INPUTDATA(input_data_no,user_id,temperature,humidity,create_date,update_date)
        VALUES (2,"chong",#{temperature},#{humidity},Now(),Now());
    </insert>
</mapper>