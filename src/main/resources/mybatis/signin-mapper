<mapper namespace="signInMapper" >
    <select id="getPassword" resultType="ac.dankook.dht2.data.User">
      SELECT user_password
      FROM USERDATA
      where user_id=#{id};
    </select>
    <insert id="insertData">
      INSERT INTO INPUTDATA(input_date_no,user_id,order_no,temperature,humidity,create_date,update_date)
      VALUES (15,"ssong",1,#{temperature},#{humidity},Now(),Now());;
    </insert>
</mapper>